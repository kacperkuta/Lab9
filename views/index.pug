html
    head
        title= title
        form(id="logout" style=visibility action=url method='POST')
            input(type="submit" value="Log out")
            input(type="hidden" name="_csrf" value=token)
    style
        include ../style.css
    body
        h1= message
        img(src="https://ocdn.eu/pulscms-transforms/1/GGtk9kpTURBXy80ZDU0OTkxN2YxMjMzMDEzYTA4ODVmYjYzYzMyOTYxNS5qcGeSlQLNA8AAwsOVAgDNA8DCw4GhMAU")
        h1="Meme list"
        ul
            each meme in memes
                li
                    a(href="meme/" + meme.id) #{meme.name}
                    img(src=meme.url)
                    p Price: #{meme.price}
        p Visited sites: #{visited}

